<div class="row">
	<div class="col-md-12">
		<div class="panel panel-default">
			<div class="panel-heading">
		       <h3 class="panel-title"><%= @project.project_name %></h3>
		    </div>
			<div class="panel-body">
				<p><strong>Client:</strong> <%= @project.client %></p>
				<p><strong>Project Manager:</strong> <%= @project.project_head.first_name %> <%= @project.project_head.last_name %></p>
				<p><strong>Description:</strong> <%= @project.description %></p>
				<p><strong>Employees:</strong></p>
				<% @project.user_projects.each do |users| %>
					<p><%= users.user.first_name %></p>
				<%end%>
			</div>
		</div>
		<%= link_to 'Back',projects_path, class:'btn btn-primary'%>
	</div>
</div>

