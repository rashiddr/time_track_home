<div class="row">
	<div class="col-md-12">
		<div class="panel panel-default">
			<div class="panel-heading">
		       <h3 class="panel-title">Status Report( <%= @daily_status.status_date %> )</h3>
		    </div>
			<div class="panel-body">
				<p><strong>Project name:</strong> <%= @project.project_name %></p>
				<p><strong>Duration:</strong> <%= @daily_status.duration %></p>
				<p><strong>Activity description:</strong></p>
				<p> <%= @daily_status.work_done %></p>
			</div>
		</div>
		<%= link_to 'Back',daily_statuses_path, class:'btn btn-primary'%>
	</div>
</div>